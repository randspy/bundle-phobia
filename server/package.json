  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec babel-node bin/www",
    "test": "jest --watchAll",
    "test-ci": "jest --ci"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/node": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "nodemon": "2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-ci"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
